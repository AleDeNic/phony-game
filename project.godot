; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="phony-game"
run/main_scene="res://scenes/world.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Audio="*res://scripts/managers/audio_manager.gd"
Notifications="*res://scripts/managers/notifications_manager.gd"
Phases="*res://scripts/managers/phase_manager.gd"
Phone="*res://scripts/managers/phone_manager.gd"
Player="*res://scripts/managers/player_manager.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
Asuka="*res://scripts/managers/asuka_manager.gd"

[dialogic]

directories/dch_directory={
"asuka": "res://characters/asuka.dch",
"self": "res://characters/self.dch",
"unit_test_character": "res://Tests/Resources/unit_test_character.dch"
}
directories/dtl_directory={
"asuka": "res://timelines/asuka.dtl",
"chat_asuka": "res://timelines/chat_asuka.dtl",
"window": "res://timelines/window.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=["res://scenes/example_style.tres"]
layout/default_style="res://scenes/example_style.tres"
variables={
"asuka": {
"chapter": "chapter_0"
},
"player_name": "name",
"window": {
"chapter": "chapter_0"
}
}
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
history/simple_history_enabled=true
history/full_history_enabled=true
history/visited_event_history_enabled=true
history/save_on_save=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://assets/ui/cursor_48.png"
mouse_cursor/custom_image_hotspot=Vector2(24, 24)

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[file_customization]

folder_colors={
"res://temp/": "red"
}

